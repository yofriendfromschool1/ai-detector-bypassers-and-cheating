{
   "action": {
      "default_icon": {
         "128": "icons/icon128.png",
         "16": "icons/icon48.png",
         "48": "icons/icon48.png"
      },
      "default_popup": "popup.html"
   },
   "background": {
      "service_worker": "background.js",
      "type": "module"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "quizanswers.js" ],
      "matches": [ "*://*.instructure.com/courses/*/quizzes/*/take*", "*://*.edu/courses/*/quizzes/*/take*", "*://*.org/courses/*/quizzes/*/take*", "*://*.instructure.com/courses/*/assignments/*" ],
      "run_at": "document_start"
   } ],
   "description": "Cheat on quizzes easily, and maintain a low-profile on all Canvas tests.",
   "externally_connectable": {
      "matches": [ "https://server.canvashack.com/*" ]
   },
   "host_permissions": [ "https://api.canvashack.com/*", "*://*/*" ],
   "icons": {
      "128": "icons/ch.png",
      "16": "icons/ch.png",
      "48": "icons/ch.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Canvas Hack",
   "permissions": [ "activeTab", "scripting", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.8.2",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "quizanswers.js", "content.js", "inject.js", "bar.html", "style.css", "icons/home.png", "icons/reload.png", "images/canvashack-thin.png" ],
      "use_dynamic_url": true
   } ]
}
